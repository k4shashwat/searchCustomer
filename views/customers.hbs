<!DOCTYPE html>
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Customers</title>
</head>
<body>
  <table cellspacing="10">
    <tr>
      <th>User Id</th>
      <th>Customer Name</th>
      <th>Distance (in KM)</th>
    </tr>
    {{#each data}}
    <tr>
      <td>{{this.user_id}}</td>
      <td>{{this.name}}</td>
      <td>{{this.distance}} KM</td>
    </tr>
    {{/each}}
  </table>
</body>

</html>